<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
      http://java.sun.com/xml/ns/javaee
      http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<!-- Usuarios -->
	<navigation-rule>
		<from-view-id>/pages/usuario/cadUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/login/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- Usuarios fim -->

	<!-- Login -->
	<navigation-rule>
		<from-view-id>/pages/login/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/aula/listAula.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
		<navigation-rule>
		<from-view-id>/pages/login/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>falha</from-outcome>
			<to-view-id>/pages/login/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/login/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>continue</from-outcome>
			<to-view-id>/pages/usuario/cadUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- Login fim -->

	<!-- Aula -->
	<navigation-rule>
		<from-view-id>/pages/aula/cadAula.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/aula/listAula.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/aula/listAula.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>atualiza</from-outcome>
			<to-view-id>/pages/aula/atualizaAula.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/aula/atualizaAula.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/aula/listAula.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/aula/listAula.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/aula/cadAula.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<!-- Aula fim -->

	<!-- Avaliacao -->
	<navigation-rule>
		<from-view-id>/pages/avaliacao/cadAvaliacao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/avaliacao/listAvaliacao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/avaliacao/cadAvaliacao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>atualiza</from-outcome>
			<to-view-id>/pages/avaliacao/atualizaAvaliacao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/avaliacao/atualizaAvaliacao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/avaliacao/listAvaliacao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/avaliacao/listAvaliacao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/avaliacao/cadAvaliacao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/aula/listAula.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>continue</from-outcome>
			<to-view-id>/pages/avaliacao/cadAvaliacao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/aula/listAula.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>lista</from-outcome>
			<to-view-id>/pages/avaliacao/listModeracao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<!-- Avaliacao fim -->

	<!-- Usuario -->
	<navigation-rule>
		<from-view-id>/pages/usuario/listUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/usuario/cadUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/usuario/listUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>atualiza</from-outcome>
			<to-view-id>/pages/usuario/atualizaUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/usuario/cadUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/usuario/listUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/usuario/atualizaUsuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/usuario/listUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- Usuarios Fim -->

	<!-- Aluno -->
	<navigation-rule>
		<from-view-id>/pages/aluno/listAluno.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/aluno/cadAluno.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/aluno/listAluno.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>atualiza</from-outcome>
			<to-view-id>/pages/aluno/atualizaAluno.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/aluno/cadAluno.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/aluno/listAluno.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/aluno/atualizaAluno.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/aluno/listAluno.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- Aluno Fim -->

	<!-- Instituicao -->
	<navigation-rule>
		<from-view-id>/pages/instituicao/listInstituicao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/instituicao/cadInstituicao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/instituicao/listInstituicao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>atualiza</from-outcome>
			<to-view-id>/pages/instituicao/atualizaInstituicao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/instituicao/cadInstituicao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/instituicao/listInstituicao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/instituicao/atualizaInstituicao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/pages/instituicao/listInstituicao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- Instituicao Fim -->

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
		<locale-config>
		</locale-config>
		<resource-bundle>
			<base-name>MessageResources</base-name>
			<var>messages</var>
		</resource-bundle>
	</application>
</faces-config>
